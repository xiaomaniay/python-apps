   "source": [
    "import numpy as np\n",
    "# Mean Absolute Deviation (MAD) \n",
    "X = np.random.randint(100, size=20)\n",
    "\n",
    "mean_X = np.mean(X)\n",
    "abs_dispersion = [np.abs(x - mean_X) for x in X]\n",
    "mad = np.sum(abs_dispersion) / len(X)\n",
    "print('Mean Absolute Deviation for X: ', mad)"
   ]
